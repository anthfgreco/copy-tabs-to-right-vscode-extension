{
  "name": "copy-tabs-to-right",
  "displayName": "Copy Tabs to Right",
  "publisher": "AnthonyGreco",
  "description": "Copy all open tabs to the right of the current tab",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copyTabsToRight.copyToClipboard",
        "title": "Copy Opened Files to Clipboard (To the Right)"
      }
    ],
    "keybindings": [
      {
        "command": "copyTabsToRight.copyToClipboard",
        "key": "ctrl+alt+]",
        "when": "editorTextFocus && !inputFocus",
        "mac": "cmd+alt+]"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
